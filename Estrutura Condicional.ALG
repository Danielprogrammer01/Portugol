Algoritmo "semnome"

Var

   numero:inteiro

Inicio

   escreval("Digite um número:")
   leia(numero)

   se (numero = 10) entao
      escreval("O número é IGUAL a 10!")
   fimse

   se (numero > 10) entao
      escreval("O número é MAIOR que 10!")
   fimse

   se (numero < 10) então
      escreval("O número é MENOR que 10!")
   fimse

Fimalgoritmo
