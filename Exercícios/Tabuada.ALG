Algoritmo "semnome"

Var

   n : inteiro

Inicio

   para n de 1 ate 10 faca
      escreval("3 x ", n, " = ", 3 * n)
   fimpara

   escreval("")

   para n de 1 ate 10 faca
      escreval("5 x ", n, " = ", 5 * n)
   fimpara

   escreval("")

   para n de 1 ate 10 faca
      escreval("7 x ", n, " = ", 7 * n)
   fimpara




Fimalgoritmo